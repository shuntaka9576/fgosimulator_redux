    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "highcharts": "^5.0.14",
    "nedb": "^1.8.0",
    "node-sass": "4.5.3",
    "photon": "github:connors/photon",
    "prop-types": "^15.5.10",
    "react": "15.4.2",
